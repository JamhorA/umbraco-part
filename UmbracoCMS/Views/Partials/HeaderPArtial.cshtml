@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
{
        var pageTitle = !String.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix offers expert financial consulting services" : "Onatrix offers expert financial consulting services";
}

    <!-- header -->
    <header class="header">
        <div class="header-container">
            <div class="top-bar-container">
                <div class="top-bar-content">
                    <span>Klarabergsviadukten 90, 111 64 Stockholm</span>
                    <div class="social-icons">
                        <a href="#"><i class="fa fa-facebook"></i></a>
                        <a href="#"><i class="fa fa-twitter"></i></a>
                        <a href="#"><i class="fa fa-linkedin"></i></a>
                    </div>
                </div>
            </div>
            <div class="header-content">
                <a href="/index.html" class="logo">
                    <img src="/images/Logo.svg" alt="Onatrix Logo">
                </a>
                <div class="navbar">
                    <nav class="header-menu">
                        <ul>
                            <li><a href="/about.html">About</a></li>
                            <li><a href="/services.html">Services</a></li>
                            <li><a href="/projects.html">Projects</a></li>
                            <li><a href="/contact.html">Contact</a></li>
                        </ul>
                    </nav>
                    <div class="search-icon">
                        <i class="fa fa-search main-search"></i>
                        <div class="header-search">
                            <form action="#" method="get" class="search-form">
                                <label for="">
                                    <input type="search" name="s" placeholder="Vad letar du efter?">
                                </label>
                                <button type="submit" class="search-btn">
                                    <i class="fa fa-search"></i>
                                </button>
                            </form>
                        </div>
                    </div>
                </div>
                <!-- Hamburgermenynsikon för mindre skärmar -->
                 <div class="menu-icon">
                    <i class="fa-solid fa-bars"></i>
                </div>
            </div>
        </div>
    </header>
    <!-- end header -->